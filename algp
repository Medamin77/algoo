ALGORITHM tab
VAR
    tab:tab1[a]
    tab:tab2[b]
    (a,b):INTEGER
 
   (i,j)INTEGER

BEGIN
 
FOR i FROM 0 TO a-1 STEP 1  DO
 read(tab1[i])
    
END_FOR
FOR i FROM0 TO b-1 STEP   DO
 read(tab2[j])
    
END_FOR
 write (tab1[i])
 write(tab2[j])
IF (tab1[i]==tab2[j]) THEN
    write(tab1[i])
END_IF
END